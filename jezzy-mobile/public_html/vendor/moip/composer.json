{
    "name": "moip/moip-sdk-php",
    "description": "Cliente em PHP para integração server-side com APIs Moip v2",
    "keywords": ["libraries", "moip", "intermediary", "checkout", "marketplace"],
    "homepage": "https://github.com/moip/moip-sdk-php",
    "license": "MIT",
    "type": "libraries",
    "authors": [
        {
            "name": "Jean Cesar Garcia",
            "email": "jeancesargarcia@gmail.com"
        },
        {
            "name": "Joao Batista Neto",
            "email": "neto.joaobatista@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "moip/moip-http-php" : "~1.0",
        "moip/moip-sdk-php": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Moip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moip\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev"
}
